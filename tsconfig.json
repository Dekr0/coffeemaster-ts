{
    "compilerOptions": {
        "target": "esnext",
        "module": "CommonJS",
        "outDir": "dist/type",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictBindCallApply": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "stripInternal": true,
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "node_modules"
    ]

}
